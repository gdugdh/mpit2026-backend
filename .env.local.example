# Example local development environment (not using Docker)
# Copy this to .env when running locally without Docker

# Server
SERVER_HOST=localhost
SERVER_PORT=8080
ENV=development

# Database (localhost for local development)
DB_HOST=localhost
DB_PORT=5432
DB_USER=dating_user
DB_PASSWORD=dating_pass
DB_NAME=dating_db
DB_SSL_MODE=disable

# Redis (localhost for local development)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT
JWT_ACCESS_SECRET=your-secret-key-change-me-minimum-32-chars
JWT_REFRESH_SECRET=your-refresh-secret-change-me-minimum-32-chars
JWT_ACCESS_EXPIRY_MIN=15
JWT_REFRESH_EXPIRY_DAY=30

# Encryption
AES_ENCRYPTION_KEY=12345678901234567890123456789012

# Storage
STORAGE_TYPE=local
STORAGE_PATH=./uploads

# Logging
LOG_LEVEL=debug
